padding-right: 4px;
padding-top: 2px;
padding-bottom: 2px;
min-height: 22px;
}

QLineEdit {
    background-color: rgb(250,250,245);
    }

QLineEdit:read-only {
    background-color: rgb(230,230,225);
    }

