QGroupBox {
color: #005045;
border: 1px solid #006020;
border-radius: 5px;
   margin-top: 0.5em;
   background-color: rgba(224,224,224,0.5);
}

QGroupBox::disabled {
color: #8A8AAE;
border: 1px solid #8A8AAE;
border-radius: 4px;
   margin-top: 0.5em;
   background-color: rgba(224,224,224,0.5);
}

QGroupBox::title {
subcontrol-origin: margin;
left: 10px;
padding: 0 3px 0 3px;
}

